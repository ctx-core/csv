{
	"name": "@ctx-core/csv",
	"version": "19.0.1",
	"description": "ctx-core csv",
	"keywords": [
		"ctx-core",
		"csv"
	],
	"homepage": "https://github.com/ctx-core/csv#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/csv/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/csv.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"_types",
		"csv__data_row",
		"csv__header_row",
		"csv__parse",
		"csv__row",
		"csv__table",
		"csv__transform_stream",
		"csv_val__hydrate",
		"header_csv__toLowerCase",
		"is_readable_stream_OR_reader",
		"reader__csv__table",
		"row__csv",
		"row_a__csv",
		"table__csv"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test:unit && pnpm test:types",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@ctx-core/table": "^24.0.1",
		"ctx-core": "^5.5.0"
	},
	"devDependencies": {
		"@types/node": "^20.10.5",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}