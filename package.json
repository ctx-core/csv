{
	"name": "@ctx-core/csv",
	"version": "14.1.72",
	"description": "ctx-core csv",
	"keywords": [
		"ctx-core",
		"csv"
	],
	"homepage": "https://github.com/ctx-core/csv#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/csv/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/csv.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": "./lib/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "^26.8.12",
		"@ctx-core/combinators": "^8.0.39",
		"@ctx-core/fetch-undici": "^2.3.55",
		"@ctx-core/function": "^21.6.0",
		"@ctx-core/nanostores": "^2.2.15",
		"@ctx-core/object": "^24.0.1",
		"@ctx-core/table": "^16.1.40",
		"papaparse": "^5.3.2",
		"svelte": "^3.55.1"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.59",
		"@swc/core": "^1.3.29",
		"@types/papaparse": "^5.3.7",
		"rimraf": "^4.1.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./lib/index.js",
	"sideEffects": false
}